# BuckBounty Environment Variables

# ==================== Agent LLM APIs ====================
# Primary LLM (OpenRouter with Claude)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Fallback LLM (Gemini 2.0 Flash)
GEMINI_API_KEY=your_gemini_api_key_here

# ==================== Financial APIs ====================
# Plaid (Banking Data)
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_sandbox_secret

# Stripe (Payments)
STRIPE_API_KEY=your_stripe_test_key

# ==================== Optional: Gmail API ====================
# For BountyHunter1 to read email coupons
# GMAIL_CLIENT_ID=your_gmail_client_id
# GMAIL_CLIENT_SECRET=your_gmail_secret

# ==================== Application ====================
# Server
PORT=8000
DEBUG=true

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
